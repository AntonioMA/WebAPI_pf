{
  "Internal Tests": {
    "allowedOrigins": [
      "^https://.*\\.telefonica\\.com/WebAPI_pf.*",
      "^https://.*\\.github\\.io/WebAPI_pf.*"
    ],
    "rules": {
      "createXMLHttpRequest": [{
        "params": {
          "options": {
            "mozSystem": true
          }
        }
      }],
      "abort": [],
      "open": [{
        "throttle": {
          "maxPetitionsPerSecond": 2
        },
        "params": {
          "method": "(GET|POST|PUT|DELETE)",
          "url": "^https://.*\\.github\\.io/WebAPI_pf.*",
          "async": true
        }
      }],
      "overrideMimeType": [{
        "params": {
          "mimeType": ".+"
        }
      }],
      "send": [{
        "type": "(ArrayBuffer|ArrayBufferView|Blob|Document|String|FormData)"
      }],
      "setRequestHeader": [{
        "params": {
          "header": ".+",
          "value": ".+"
        }
      }],
      "onreadystatechange": [],
      "set": [
        "response",
        "responseText",
        "responseXML",
        "status",
        "statusText",
        "readyState",
        "timeout",
        "responseURL"
      ],
      "onabort": [],
      "onerror": [],
      "onload": [],
      "onloadend": [],
      "onloadstart": [],
      "onprogress": [],
      "ontimeout": [],
      "addEventListener": [],
      "removeEventListener": [],
      "dispatchEvent": []
    }
  }
}
